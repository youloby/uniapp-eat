
.custom-tabbar {
     border-bottom: 1rpx solid #bbb;
     position: fixed;
     top: 0;
     left: 0;
     right: 0;
     z-index: 10001;
     overflow: hidden;
}
.tabbar {
         display: -webkit-box;
         display: -webkit-flex;
         display: flex;
         -webkit-flex-wrap: nowrap;
                 flex-wrap: nowrap;
         -webkit-justify-content: space-around;
                 justify-content: space-around;
}
.tabbar-list {
         height: 100%;
         text-align: center;
         font-size: 28rpx;
}
.custom-tabbar-vertical {
     border-right: 1rpx solid #eee;
     background-color: #fff;
     position: fixed;
     top: 0;
     left: 0;
     z-index: 10001;
     overflow: hidden;
}
.tabbar-vertical {
}
.tabbar-list-vertical {
             width: 100%;
             text-align: center;
}


