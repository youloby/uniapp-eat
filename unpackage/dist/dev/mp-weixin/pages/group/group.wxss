.group-container {
  margin: auto;
  background-color: #f9f9f9;
}
.group-container .kefu {
  margin: 10rpx;
  padding: 10rpx;
  background-color: #fff;
  width: 800rpx;
  height: 169rpx;
}
.group-container .kefu image {
  width: 100%;
  height: 100%;
}
.group-container .info {
  margin: auto;
  background-color: #fff;
  padding: 10rpx;
  font-size: 24rpx;
  width: 700rpx;
  height: 169rpx;
}
.group-container .type_area {
  font-size: 24rpx;
  width: 800rpx;
}
.group-container .type_area .title .vip {
  color: #FF0000;
  font-size: 28rpx;
}
.group-container .type_area .headline1 {
  margin: 10px auto;
  font-weight: 700;
  font-size: 30rpx;
}
.group-container .type_area .headline1 .img {
  width: 700rpx;
  height: 400px;
}
.group-container .type_area .headline1 .img image {
  width: 100%;
  height: 100%;
}

